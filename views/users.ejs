<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Users</title>
  <link rel="stylesheet" href="../public//users.css">
</head>
<body>
  <h2>Users</h2>

  <table border="1">
    <tr>
      <th>Id</th>
      <th>FirstName</th>
      <th>LastName</th>
      <th>Age</th>
      <th>Update</th>
      <th>Delete</th>
    </tr>

    <% if (data.length) { %>
     <% data.forEach((item) => { %>
        <tr>
          <td><%= item.id %></td>
          <td><%= item.firstName %></td>
          <td><%= item.lastName %></td>
          <td><%= item.age %></td>
          <td><a href="/updateuser/?id=<%= item.id %>">Update</a></td>
          <td><a href="/deleteuser/?id=<%= item.id %>">Delete</a></td>
        </tr>
     <% }) %>
    <% } %>
  </table>
</body>
</html>